  <elk:keymap name="#logical-uk" description="Logical UK keyboard">
    <!-- | next to Z to shift right-->
    <elk:keydef winkey="Oem5" shift="True">
      <elk:keymatrix col="0" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- \ next to Z to ctrl right-->
    <elk:keydef winkey="Oem5" shift="False">
      <elk:keymatrix col="0" row="0" />
      <elk:keymatrix col="13" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Right">
      <elk:keymatrix col="0" row="0" />
    </elk:keydef>
    <!-- non shifted [ to shift copy == [-->
    <elk:keydef winkey="OemOpenBrackets" shift="false">
      <elk:keymatrix col="0" row="1" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- non shifted ] to ctrl copy == ]-->
    <elk:keydef winkey="Oem6" shift="false">
      <elk:keymatrix col="0" row="1" />
      <elk:keymatrix col="13" row="2" />
    </elk:keydef>
    <elk:keydef winkey="End">
      <elk:keymatrix col="0" row="1" />
    </elk:keydef>
    <elk:keydef winkey="None">
      <elk:keymatrix col="0" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Space">
      <elk:keymatrix col="0" row="3" />
    </elk:keydef>


    <!-- shift-6 to shift left == ^ -->
    <elk:keydef winkey="D6" shift="true">
      <elk:keymatrix col="1" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- shift-# to ctrl left == ~ -->
    <elk:keydef winkey="Oem7" shift="true">
      <elk:keymatrix col="1" row="0" />
      <elk:keymatrix col="13" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Left">
      <elk:keymatrix col="1" row="0" />
    </elk:keydef>
    <!-- shift-minus to shift down == _ -->
    <elk:keydef winkey="OemMinus" shift="true">
      <elk:keymatrix col="1" row="1" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- shift-] to shift ctrl == } -->
    <elk:keydef winkey="Oem6" shift="true">
      <elk:keymatrix col="1" row="1" />
      <elk:keymatrix col="13" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Down">
      <elk:keymatrix col="1" row="1" />
    </elk:keydef>
    <elk:keydef winkey="Return">
      <elk:keymatrix col="1" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Delete">
      <elk:keymatrix col="1" row="3" />
    </elk:keydef>
    <elk:keydef winkey="Back">
      <elk:keymatrix col="1" row="3" />
    </elk:keydef>
    
    
    <elk:keydef winkey="Subtract">
      <elk:keymatrix col="2" row="0" />
    </elk:keydef>
    <elk:keydef winkey="OemMinus" shift="false">
      <elk:keymatrix col="2" row="0" />
    </elk:keydef>
    <!-- shift-= to shift-minus =-->
    <elk:keydef winkey="OemPlus" shift="false">
      <elk:keymatrix col="2" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>

    <!-- shift-3 to shift up == £ -->
    <elk:keydef winkey="D3" shift="true">
      <elk:keymatrix col="2" row="1" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- shift [ to ctrl up == {-->
    <elk:keydef winkey="OemOpenBrackets" shift="true">
      <elk:keymatrix col="2" row="1" />
      <elk:keymatrix col="13" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Up">
      <elk:keymatrix col="2" row="1" />
    </elk:keydef>

    <!-- shift-8 to shift-: *-->
    <elk:keydef winkey="D8" shift="true">
      <elk:keymatrix col="2" row="2" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- shift-: to :-->
    <elk:keydef winkey="Oem1" shift="true">
      <elk:keymatrix col="2" row="2" />
      <elk:keymatrix col="13" row="3" force="off" /> 
    </elk:keydef>
    <elk:keydef winkey="None">
      <elk:keymatrix col="2" row="3" />
    </elk:keydef>
    
    
    <!-- shift-' to shift-0 = @-->
    <elk:keydef winkey="Oem3" shift="true">
      <elk:keymatrix col="3" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <elk:keydef winkey="D0" shift="false">
      <elk:keymatrix col="3" row="0" />
    </elk:keydef>
    <elk:keydef winkey="P">
      <elk:keymatrix col="3" row="1" />
    </elk:keydef>
    
    <!-- shift-= to shift-semi = +-->
    <elk:keydef winkey="OemPlus" shift="true">
      <elk:keymatrix col="3" row="2" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <!-- ; -->
    <elk:keydef winkey="Oem1" shift="false">
      <elk:keymatrix col="3" row="2" />
    </elk:keydef>
    <elk:keydef winkey="Oem2">
      <elk:keymatrix col="3" row="3" />
    </elk:keydef>

    <!-- shift-0 to shift-9 = )-->
    <elk:keydef winkey="D0" shift="true">
      <elk:keymatrix col="4" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <elk:keydef winkey="D9" shift="false">
