      <elk:keymatrix col="4" row="2" />
    </elk:keydef>
    <elk:keydef winkey="OemPeriod">
      <elk:keymatrix col="4" row="3" />
    </elk:keydef>


    <!-- shift-9 to shift-8 = (-->
    <elk:keydef winkey="D9" shift="true">
      <elk:keymatrix col="5" row="0" />
      <elk:keymatrix col="13" row="3" />
    </elk:keydef>
    <elk:keydef winkey="D8" shift="false">
